                            <meta itemprop="mpn" content="921528999" />
                            <meta itemprop="sku" content="921528999" />
                            <meta itemprop="priceValidUntil" content="2022.05.29">


                            <a class="button__yellow modal_form_selector"
                               href="#login-form"
                               rel="modal:open">Замовити</a>
