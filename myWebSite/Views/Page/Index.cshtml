@{
    string NameCompany = "point";
    string domen = ViewBag?.Domen ?? "";
}


<header class="header" style="display: none;">
    <button class="header__search-icon show-for-medium" id="search-btn">
        <i class="fas fa-search"></i>
        <i class="fas fa-times"></i>
    </button>
</header>
<div class="contact_form_wrapper modal" id="login-form">
    <form asp-controller="Message" asp-action="Send" class="contact_form">
        <div class="contact_form_block form-group">
            <input type="text" id="modal_form_name" name="name" placeholder="Ваше ім'я">
            <span class="contact_form_error form-text text-muted">Будь ласка, введіть Ваше ім'я</span>
        </div>
        <div class="contact_form_block form-group">
            <input type="tel" id="modal_form_phone" name="telegram" placeholder="Ваш telegram">
            <span class="contact_form_error form-text text-muted">Будь ласка, введіть Ваш телеграм(телефон чи юзернейм)</span>
        </div>
        <button class="contact_form_submit modal_form_submit " id="modal_form_submit">Замовити</button>
    </form>

</div>
<main class="service__main" itemscope itemtype="http://schema.org/Product">
